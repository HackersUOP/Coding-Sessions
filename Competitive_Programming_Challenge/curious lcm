*********************************************************
Authour's sloution.cpp
---------------------------------------------------------

#include <bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin >> t;
    while(t--){
        int n;
        cin>>n;
        int mx = 1;
        for(int i=2; i*i<=n; ++i)
            if(n%i==0) mx=max(mx,n/i);
        cout << mx << " " << n-mx << endl;    
    }
}

*********************************************************

At the competition time : by e17_praditha solution.py
---------------------------------------------------------

T = int(input())
for t in range(T):
    n = int(input())
    found = False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            found = True
            print(n//i, n - n//i)
            break
    if not found:
        print(1, n - 1)

